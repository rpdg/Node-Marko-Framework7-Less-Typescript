{
  "plugins": [
    "lasso-marko","lasso-less"
  ],
  "outputDir": "build/static/",
  "fingerprintsEnabled": false,
  "urlPrefix": "",
  "minify": false,
  "resolveCssUrls": true,
  "bundlingEnabled": true,
  "bundles": [
    {
      "name": "framework7IOSCSS",
      "dependencies": [
        {
          "path": "framework7/dist/css/framework7.ios.css",
          "slot": "ios-slot1"
        },
        {
          "path": "framework7/dist/css/framework7.ios.colors.css",
          "slot": "ios-slot1"
        }
      ]
    },
    {
      "name": "framework7MaterialCSS",
      "dependencies": [
        {
          "path": "framework7/dist/css/framework7.material.css",
          "slot": "material-slot1"
        },
        {
          "path": "framework7/dist/css/framework7.material.colors.css",
          "slot": "material-slot1"
        }
      ]
    },
    {
      "name": "AppIOSCSS",
      "dependencies": [
        {
          "path": "src/layout/css/layout-ios.less",
          "slot": "my-ios-slot1"
        }
      ]
    },
    {
      "name": "AppMaterialCSS",
      "dependencies": [
        {
          "path": "src/layout/css/layout-android.less",
          "slot": "my-material-slot1"
        }
      ]
    }
  ]
}